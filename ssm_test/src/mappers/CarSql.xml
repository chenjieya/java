<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.CarDao">
    <insert id="insertCar">
        INSERT INTO t_car(cname,color,price) values(#{cname},#{color},#{price});
    </insert>

    <select id="selectCar" resultType="domain.Car">
        select cno,cname,color,price from t_car;
    </select>
</mapper>